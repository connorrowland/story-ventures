  "private": true,
  "dependencies": {
    "clipboard": "^2.0.1",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.3.3",
    "normalize.css": "^8.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "react-scripts": "1.1.5",
